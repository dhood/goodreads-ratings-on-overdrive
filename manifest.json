{
  "manifest_version": 2,
  "name": "Goodreads rating on Overdrive",
  "version": "0.1",
  "description": "Show Goodreads ratings on Overdrive",
  "content_scripts": [
  {
    "matches": [
      "https://*.overdrive.com/*"
    ],
    "js": ["content.js"]
  }
  ],
  "web_accessible_resources": [
      "config.js"
  ],
  "permissions": [
      "https://*.overdrive.com/*"
  ]
}
